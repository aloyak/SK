import units

let distance = [15..20]m
let time = 5s

print(kind(time))
print(distance / time) // [3..4] m/s


units.define("mph", 0.44704 m/s) // Defined mph as 0.45 m/s
print(10 mph) // returns 4.4704 m/s


print(10 mph + 10 m/s) // Totally allowed!
print(5m * 3m^2) // Area to volume

