{
  "name": "sk-lang",
  "displayName": "SK Programming Language Support",
  "description": "The SK Programming Language VSCode Extension!",
  "version": "0.6.1",
  "publisher": "aloyak",
  "license": "MIT",
  "icon": "./skicon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/aloyak/SK"
  },
  "engines": {
    "vscode": "^1.104.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sk",
      "aliases": ["SK", "sk"],
      "extensions": [".sk"],
      "configuration": "./language-configuration.json",
      "icon": {
          "light": "skfileicon.png",
          "dark": "skfileicon.png"
        }
    }],
    "grammars": [{
      "language": "sk",
      "scopeName": "source.sk",
      "path": "./syntaxes/sk.tmLanguage.json"
    }],
    "snippets": [
      {
        "language": "sk",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
